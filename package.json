{
  "name": "startwars-name",
  "version": "0.0.0-semantically-released",
  "description": "Get random Star Wars names",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "mocha src/index.test.js -w",
    "test:single": "mocha src/index.test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/diego-oliveira/startwars-name.git"
  },
  "keywords": [
    "random",
    "star",
    "wars",
    "name"
  ],
  "author": "Diego Oliveira <oliveira.bsi@gmail.com> (http://doliveira.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/diego-oliveira/startwars-name/issues"
  },
  "homepage": "https://github.com/diego-oliveira/startwars-name#readme",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "commitizen": "2.8.1",
    "cz-conventional-changelog": "1.1.6",
    "ghooks": "1.2.1",
    "mocha": "2.5.3",
    "semantic-release": "^4.3.5"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single"
    }
  }
}
